<h3>
    List of Employees
</h3>

 <% for (let employee of employees) { %>
    <li>
        Emp Id: <%= employee.id %>
        <br>
        Full name : <%= employee.first_name %> <%= employee.last_name %>
        <br>
        City: <%= employee.city %>
        <br>
        Occupation <%= employee.occupation %>
    </li>
    <hr>
<% } %>